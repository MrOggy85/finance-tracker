{
  "name": "finance-tracker",
  "version": "0.1.0",
  "private": true,
  "main": "src/main.ts",
  "build": {
    "appId": "com.otaku.finance-tracker",
    "files": [
      "arc/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "postinstall": "install-app-deps",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "tsc --project electron/tsconfig.json && electron ./build/electron/main.js",
    "electron:build": "tsc --project electron/tsconfig.json",
    "electron:watch": "tsc --watch --project electron/tsconfig.json"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bootstrap": "^5.1.1",
    "concurrently": "^6.0.0",
    "electron": "^12.0.0",
    "electron-is-dev": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-open-weather": "^1.1.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "reactstrap": "^8.10.0",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.37",
    "victory": "^36.0.0",
    "wait-on": "^5.2.1",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.2",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.9",
    "@types/sqlite3": "^3.1.7",
    "electron-builder": "^22.11.7",
    "typescript": "^4.4.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
